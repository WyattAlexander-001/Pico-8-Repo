pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
--core functions

function _init()
	map_setup()
	make_player()
end

function _update60() 
	move_player()
end

function _draw()
 cls()
 draw_map()
 draw_player()
 if (btn(❎)) show_inventory()
 debug_screen()
end

 
-->8
--camera/map
function map_setup()
	wall=0
	key =1
	door=2
end

function draw_map()
	mapx=flr(p.x/16)*16
	mapy=flr(p.y/16)*16
	camera(mapx*8,mapy*8)
	map(0,0,0,0,128,64) --draw whole map
end

function is_tile(tile_type,x,y)
	tile=mget(x,y)
	has_flag=fget(tile,tile_type)
	return has_flag
end

function can_move(x,y)
	return not is_tile(wall,x,y)
end

function swap_tile(x,y)
	tile=mget(x,y)
	mset(x,y,tile+1) --key:blank in sprite sheet
end

function get_key(x,y)
	p.keys+=1
	swap_tile(x,y)
	sfx(1)
end
-->8
--player
function make_player()
	p={}
	p.x=45
	p.y=23
	p.sprite=1
	p.keys=0
	p.direction=false
end

function draw_player()
	spr(p.sprite, p.x*8,p.y*8,1,1,p.direction)
end

function move_player()
	newx=p.x
	newy=p.y
	if (btnp(⬅️)) then
		p.direction=true
	 newx-=1
	end
	if (btnp(➡️)) then
		p.direction=false
	 newx+=1
	end
	if (btnp(⬆️)) newy-=1
	if (btnp(⬇️)) newy+=1
	
	interact(newx,newy)
	
	if(can_move(newx,newy)) then
		p.x=mid(0,newx,127)
		p.y=mid(0,newy,63)
	else
		sfx(0)
	end
end

function interact(x,y)
	if (is_tile(key,x,y)) then
		get_key(x,y)
	end
end
-->8
--inventory

function show_inventory()
	--default coordinates lower left
	invx=mapx*8+0 
	invy=mapy*8+105
	
	rectfill(invx,invy,invx+100,invy+20,0) --black box
	print("inventory: ",invx+20,invy+4,9) --9 is orange
	print("keys: " ..p.keys,invx+20,invy+11,10) -- 10 is yellow
end
-->8
--utility
function truesgn(a) -- -5 returns -1
    local r=0
    if (a!=0) r=sgn(a)
    return r
end

function debug_screen()
	invx=mapx*8+0 
	invy=mapy*8+0
		
	print("debug menu: " ,invx+0,invy+1,8)
	print("x: "..p.x ,invx+0,invy+10,8)		
	print("y: "..p.y ,invx+0,invy+20,8)

end
