pico-8 cartridge // http://www.pico-8.com
version 42
__lua__

--player coordinates
player_x = 0
player_y = 0
is_facing_left = false


--players
characters = {
	{sprite = 1, power = "bomb"},
	{sprite = 2, power = "laser"},
	{sprite = 3, power = "nothing"}
}

current_char = 1

--swapping char delay:
swap_delay = 0
swap_cooldown = 6

--movement speed delay:
move_delay = 0
move_cooldown = 6 

function handle_swap()
	if swap_delay == 0 then
		current_char = current_char % #characters + 1
		swap_delay = swap_cooldown
	end
end	

function swap_delay_timer()
	if swap_delay > 0 then
		swap_delay -= 1
	end
end


--core functions
function _init()
	player_x = 1
	player_y = 14
	move_delay = move_cooldown
end

function _update60()
 -- handle swap delay timer
 swap_delay_timer()
    
 -- movement delay timer
 if move_delay > 0 then
     move_delay -= 1
 end

 -- check for movement buttons and apply delay
 if move_delay == 0 then
     if btn(0) then 
        	is_facing_left = true
         player_x -= 1  -- move left
         move_delay = move_cooldown
     elseif btn(1) then 
        	is_facing_left = false
         player_x += 1  -- move right
         move_delay = move_cooldown
     elseif btn(2) then 
         player_y -= 1  -- move up
         move_delay = move_cooldown
     elseif btn(3) then 
         player_y += 1  -- move down
         move_delay = move_cooldown
     end
 end

 -- handle character swap
 if (btn(4)) then
		handle_swap()
 end 
end


function _draw()
 cls()

 -- screen size in pixels
 local screen_width = 128
 local screen_height = 128

 -- determine the offsets for drawing the map based on the character's position
 local map_x_offset = max(0, min(player_x - screen_width / 16, 128 - screen_width / 8))
 local map_y_offset = max(0, min(player_y - screen_height / 16, 64 - screen_height / 8))

 -- draw the map section based on the offsets
 map(map_x_offset, map_y_offset, 0, 0, screen_width / 8, screen_height / 8)

 -- calculate character's screen position
 -- this allows the character to move to the edges of the screen
 local char_screen_x = (player_x - map_x_offset) * 8
 local char_screen_y = (player_y - map_y_offset) * 8

 -- draw the character at the calculated screen position
 local char = characters[current_char]
	spr(char.sprite, char_screen_x, char_screen_y, 1, 1, is_facing_left)
end


 
-->8
function character()
end

-->8
function enemy()
end
