pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
--my first game
function _init()
	print ("hello") 
	x=64
	y=64
end

function _update()
//player controls:
	if(btn(0)) x-=1//0 = <- arrow
	if(btn(1)) x+=1
	if(btn(2)) y-=1
	if(btn(3)) y+=1
	
//sound effect
	if(btn(5)) sfx(0) //5 is x
	
end

function _draw()
	cls()//clears screen
	map(0,0)
	spr(1,x,y)//sprite 1, pos(x,y)
end 
